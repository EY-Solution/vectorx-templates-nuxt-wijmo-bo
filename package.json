{
    "name": "@vectorx/guide",
    "version": "1.0.2",
    "description": "VectorX node framework guide",
    "author": "Roy Lee <keymasroy@gmail.com>",
    "license": "MIT",
    "type": "module",
    "private": true,
    "scripts": {
        "clean": "nuxi cleanup",
        "dev": "nuxt dev -p 21001",
        "generate": "yarn run lint && yarn run typecheck && auto-version --patch && cross-env CONFIG_ENV=dev nuxt generate",
        "lint": "eslint .",
        "postinstall": "node ./scripts/run.js ./install.mjs && nuxt prepare",
        "typecheck": "vue-tsc --noEmit && nuxi typecheck docs"
    },
    "imports": {
        "#vectorx/core": "@vectorx/core/src",
        "#vectorx/core/*": "@vectorx/core/src/*",
        "#vectorx/h3": "@vectorx/h3/src",
        "#vectorx/h3/*": "@vectorx/h3/src/*",
        "#vectorx/nuxt": "@vectorx/nuxt/src",
        "#vectorx/nuxt/*": "@vectorx/nuxt/src/*",
        "#vectorx/nuxt-wijmo": "@vectorx/nuxt-wijmo/src",
        "#vectorx/nuxt-wijmo/*": "@vectorx/nuxt-wijmo/src/*",
        "#vectorx/nuxt-ckeditor5": "@vectorx/nuxt-ckeditor5/src",
        "#vectorx/nuxt-ckeditor5/*": "@vectorx/nuxt-ckeditor5/src/*"
    },
    "resolutions": {
        "@vectorx/core/@vectorx/types": "./.vectorx/vectorx-types-v24.1.5-alpha.43.tgz",
        "@vectorx/nuxt/@vectorx/core": "./.vectorx/vectorx-core-v24.1.5-alpha.43.tgz",
        "@vectorx/nuxt-vuetify/@vectorx/core": "./.vectorx/vectorx-core-v24.1.5-alpha.43.tgz",
        "@vectorx/nuxt-vuetify/@vectorx/nuxt": "./.vectorx/vectorx-nuxt-v24.1.5-alpha.43.tgz",
        "@vectorx/nuxt-wijmo/@vectorx/core": "./.vectorx/vectorx-core-v24.1.5-alpha.43.tgz",
        "@vectorx/nuxt-wijmo/@vectorx/nuxt": "./.vectorx/vectorx-nuxt-v24.1.5-alpha.43.tgz",
        "@vectorx/nuxt-wijmo/@vectorx/nuxt-vuetify": "./.vectorx/vectorx-nuxt-vuetify-v24.1.5-alpha.43.tgz",
        "@vectorx/nuxt-ckeditor5/@vectorx/nuxt": "./.vectorx/vectorx-nuxt-v24.1.5-alpha.43.tgz"
    },
    "devDependencies": {
        "@nuxt/eslint-config": "^0.3.13",
        "@std/esm": "^0.26.0",
        "@types/shelljs": "^0.8.15",
        "@typescript-eslint/eslint-plugin": "^7.7.1",
        "@typescript-eslint/parser": "^7.7.1",
        "@vectorx/core": "./.vectorx/vectorx-core-v24.1.5-alpha.43.tgz",
        "@vectorx/nuxt": "./.vectorx/vectorx-nuxt-v24.1.5-alpha.43.tgz",
        "@vectorx/nuxt-vuetify": "./.vectorx/vectorx-nuxt-vuetify-v24.1.5-alpha.43.tgz",
        "@vectorx/nuxt-wijmo": "./.vectorx/vectorx-nuxt-wijmo-v24.1.5-alpha.43.tgz",
        "@vectorx/nuxt-ckeditor5": "./.vectorx/vectorx-nuxt-ckeditor5-v24.1.5-alpha.43.tgz",
        "@vectorx/types": "./.vectorx/vectorx-types-v24.1.5-alpha.43.tgz",
        "auto-version-js": "^0.3.10",
        "eslint": "^8.57.0",
        "prettier": "3.2.5",
        "shelljs": "^0.8.5",
        "ts-node": "^10.9.2",
        "vue-tsc": "^2.0.24"
    },
    "dependencies": {}
}